  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "start:server": "babel-node server",
    "start": "cross-env NODE_OPTIONS=--max_old_space_size=8192 NODE_ENV=development babel-watch server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true babel-watch server",
    "prod": "npm run build && npm run buildServer && npm run start:prod",
    "start:prod": "cross-env NODE_OPTIONS=--max_old_space_size=8192 NODE_ENV=production node build/server",
    "presetup": "npm i chalk shelljs",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write",
    "buildServer": "rimraf ./build/server && babel server -s -d build/server"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
